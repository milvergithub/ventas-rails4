<div class="col-xs-6 col-xs-offset-3">
  <div class="list-group-item active">Create/update usuario</div>
  <legend></legend>
  <%= form_for (@usuario), url: {action: "update"}, html: {class: ""} do |user| %>
      <div class="form-group">
        <label for="">nombres:</label>
        <%= user.text_field :nombres, class: "form-control" %>
      </div>
      <div class="form-group">
        <label for="">Apellido Paterno:</label>
        <%= user.text_field :ape_pat, class: "form-control" %>
      </div>
      <div class="form-group">
        <label for="">Apellido Materno:</label>
        <%= user.text_field :ape_mat, class: "form-control" %>
      </div>
      <div class="form-group">
        <label for="">Email:</label>
        <%= user.text_field :email, class: "form-control" %>
      </div>
      <div class="form-group">
        <label for="">Telefono:</label>
        <%= user.text_field :telefono, class: "form-control" %>
      </div>
      <div class="form-group">
        <label for="">Fecha Nac:</label>
        <%= user.text_field :fecha_nacimiento, class: "form-control datepicker" %>
      </div>
      <div class="form-group">
        <label for="">Usuario:</label>
        <%= user.text_field :login, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= user.submit "Update", class: "btn btn-success" %>
      </div>
  <% end %>
</div>